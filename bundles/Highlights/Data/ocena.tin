#sub {(posluzy krotko, w znakomitym stanie)} {<018>posluzy krotko - 2/8 1h+<088>};
#sub {(posluzy krotko)} {<018>posluzy krotko - 2/8 1h+<088>};

#sub {(posluzy bardzo krotko, w znakomitym stanie)} {<018>posluzy bardzo krotko - 1/8 <1h<088>};
#sub {(posluzy bardzo krotko)} {<018>posluzy bardzo krotko - 1/8 <1h<088>};

#high {(nie posluzy dluzej, w znakomitym stanie)} {<018>};

#sub {ledwo          } {<018>ledwo        10<088>};
#sub {troche         } {<018>troche       20<088>};
#sub {pobieznie      } {<018>pobieznie    30<088>};
