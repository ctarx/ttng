#nop opisy pojemnikow;
#sub {$ \(} {<138>$ <088>\(};
#nop poziomy zuzycia;
#high {(w znakomitym stanie)} {<118><afa>};
#high {(lekko podniszczon%w)} {<038>};

#high {(w kiepskim stanie)} {<018>};
#high {(w oplakanym stanie)} {<118>};
#high {(w dobrym stanie)} {<028>};
#high {(liczne walki wyryly na niej swoje pietno)} {<038>};
#high {(liczne walki wyryly na nim swoje pietno)} {<038>};
#high {(w bardzo zlym stanie)} {<018>};
#high {(w zlym stanie)} {<018>};
#high {(wymaga natychmiastowej konserwacji)} {<118>};

#nop poziomy magicznego zuzycia;
#sub {(posluzy naprawde dlugo, w znakomitym stanie)} {<028>posluzy bardzo dlugo - 8/8 8d+<088>};
#sub {(posluzy naprawde dlugo)} {<028>posluzy bardzo dlugo - 8/8 8d+<088>};

#sub {(posluzy bardzo dlugo, w znakomitym stanie)} {<028>posluzy bardzo dlugo - 7/8 5d+<088>};
